(module
  (type $f (;0;) (func))
  (type $k (;1;) (cont $f))
  (tag $exn (;0;) (type $f))
  (export "throw_never" (func 1))
  (elem (;0;) declare func $never)
  (func $never (;0;) (type $f)
    unreachable
  )
  (func (;1;) (type $f)
    block $h
      try_table (catch $exn $h) ;; label = @2
        ref.func $never
        cont.new $k
        resume_throw $k $exn
        unreachable
      end
    end
  )
)
