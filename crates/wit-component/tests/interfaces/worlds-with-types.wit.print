package foo:foo;

interface import-me {
  type foo = u32;
}

world simple {
  import a: func(a: foo) -> bar;

  record foo {
    f: u8,
  }

  type bar = foo;

  export b: func(a: foo) -> bar;
}
world with-imports {
  import import-me;
  import a: func(a: foo);
  use import-me.{foo};

  export b: func(a: foo);
}
